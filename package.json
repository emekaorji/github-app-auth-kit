{
  "name": "github-app-auth-kit",
  "version": "0.0.6",
  "type": "module",
  "description": "TypeScript helpers for GitHub App JWTs and installation access tokens.",
  "repository": {
    "type": "git",
    "url": "https://github.com/emekaorji/github-app-auth-kit"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "bump": "npm --no-git-tag-version version -f",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "github-app",
    "installation-token",
    "jwt",
    "typescript"
  ],
  "author": "Emeka Orji",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsup": "^8.5.1",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  }
}
